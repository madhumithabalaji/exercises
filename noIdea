import sys 

lines = sys.stdin.readlines()
input = [i for i in lines[1].strip().split(' ')]
a = [i for i in lines[2].strip().split(' ')]
b = [i for i in lines[3].strip().split(' ')]

total = 0
for i in input:
    if(i in a):
        total = total + 1
        exit
    if(i in b):
        total = total - 1
print(total)
